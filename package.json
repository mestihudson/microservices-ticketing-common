  },
  "devDependencies": {
    "del-cli": "^4.0.1",
    "nodemon": "^2.0.15",
    "tsc-alias": "^1.4.1",
    "typescript": "^4.4.4"
  },
  "dependencies": {
    "@types/cookie-session": "^2.0.43",
    "@types/express": "^4.17.13",
    "@types/jsonwebtoken": "^8.5.6",
    "cookie-session": "^1.4.0",
    "express": "^4.17.1",
    "express-validator": "^6.13.0",
    "jsonwebtoken": "^8.5.1",
    "node-nats-streaming": "^0.3.2"
  },
  "author": {
    "name": "mestihudson",
    "url": "https://github.com/mestihudson"
  },
